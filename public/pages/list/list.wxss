.list-body{
    box-sizing: border-box;
    background: #383b4b;
}
.list-tabs {
    display: flex;
    flex: 1;
}
.list-tab {
    display: flex;
    flex: 1;
    justify-content: center;
    background: #252432;
    color: #605f6f;
    font-size: 12px;
    line-height: 44px;
}
.refresh{
    text-align:center;
    line-height: 44px;
    font-size:14px;
    color: #838383;
    overflow: hidden;
}
.item{
    box-sizing: border-box;
    padding: 10px 10px 0 10px;
    width: 100%;
    /*border-bottom: 1px solid #f8f8f8;*/
}
.item-left{
    float: left;
    max-width: 100%;
    text-overflow: ellipsis;
    overflow: hidden;
    width:80px;
    height: 80px;
    border-radius: 5px;
    background: #e1e1e1
}
.item-left image{
    width: 100%;
    height: 100%;
}
.item-right{
    float: left;
    /*margin-left: 10px;*/
    padding-bottom: 10px;
    width: 100%;
    /*height: 80px;*/
    border-bottom: 1px solid rgba(0,0,0,.2);
    box-sizing: content-box;
}
.item-title{
    display: block;
    font-size:14px;
    color:#fff;
    line-height: 40px;
}
.item-des{
    display: block;
    font-size:12px;
    line-height: 20px;
    color:#4a4a52;
    width: 100%;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.search-projects{
    width: 100%;
    padding:5px 10px;
    text-align: center;
    background-color: #21212d;
}
.search-projects-body{
    width:100%;
    background: #000;
    height: 30px;
    border-radius: 3px;
    box-sizing: border-box;
    padding: 0 30px 0 0;
}
.search-input input {
    width:100%;
    line-height: 30px;
    height: 30px;
    border-radius: 3px;
    font-size:12px;
    box-sizing: border-box;
    text-align:left;
}

.search-input{
    position: relative;
    width: 80px;
    padding-left: 30px;
    height: 30px;
    box-sizing: border-box;
    display: inline-block;
    color: #fff;

}
.search-icon{
    display: inline-block;
    position: absolute;
    left: 5px;
    top: 5px;
    width: 20px;
    height: 20px;
    z-index: 100;
}
.clear-icon{
    display: inline-block;
    position: absolute;
    right: -20px;
    top: 5px;
    width: 20px;
    height: 20px;
    z-index: 100;
}


