.list-body{
    box-sizing: border-box;
    /* background: #383b4b; */
    /* background: #000; */
    /* background:#383B4A; */
}
.list-tabs {
    display: flex;
    flex: 1;
}
.list-tab {
    display: flex;
    flex: 1;
    background: #252525;
    color: #605f6f;
    font-size: 14px;
    line-height: 44px;
    padding-left: 10px;
    justify-content: left;
}
.refresh{
    text-align:center;
    line-height: 44px;
    font-size:14px;
    color: #838383;
    overflow: hidden;
}
.item{
    box-sizing: border-box;
    width: 100%;
    flex-direction: row;
    position: relative;
    height:74px;
}
.item-content{
    width: 100%;
    display: flex;
    position: absolute;
    left:0;
    top:0;
    z-index: 15;
    height:74px;
    padding: 15px 12px;
    background: #010101;
    border-bottom: 1px solid #14161A;
}
.item-operate{
  width: 100%;
  display: flex;
  left:0;
  top:0;
  z-index: 0;
  position:absolute;
  height:74px;
}
.item-operate-btn{
  position: absolute;
  width: 76px;
  height:74px;
  background-color: #9B9B9B;
  font-size: 15px;
  color: #FFFFFF;
  letter-spacing: 1.29px;
  padding:28px 20px;
  right:0;
}
.item-left{
    overflow: hidden;
    width:46px;
    height: 46px;
    border-radius: 3px;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #1166EA;
}
.item-left > view{
    width: 100%;
    height: 100%;
}
.item-left image{
    width:100%;
    height: 100%;
}
.item-left .item-left-text{
    display: flex;
    justify-content: center;
    align-items: center;
    height: 100%;
    /* width: px;
    height: 24px; */
    text-align: center;
    font-size: 20px;
    color: #FFFFFF;
    overflow: hidden;
}
.item-right{
    margin-left: 8px;
    max-width: 240px;
}
.item-title{
    display: block;
    font-size:14px;
    color:#fff;
    line-height: 28px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.item-des{
    display: block;
    font-size:12px;
    color: #9B9B9B;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.search-projects{
    width: 100%;
    padding:9px 12px;
    text-align: center;
    background-color: #171D29;
}
.search-projects-body{
    width:100%;
    background: #263044;
    border-radius: 100px;
    height: 30px;
    box-sizing: border-box;
    padding: 0 30px 0 0;
}
.search-input input {
    width:100%;
    line-height: 30px;
    height: 30px;
    /* border-radius: 100px; */
    font-size:13px;
    box-sizing: border-box;
    text-align:left;
    color: #ACACAC;
}

.search-input{
    position: relative;
    width: 80px;
    padding-left: 30px;
    height: 30px;
    box-sizing: border-box;
    display: inline-block;
    color: #fff;

}
.search-icon{
    display: inline-block;
    position: absolute;
    left: 5px;
    top: 5px;
    width: 20px;
    height: 20px;
    z-index: 100;
}
.clear-icon{
    display: inline-block;
    position: absolute;
    right: -20px;
    top: 5px;
    width: 20px;
    height: 20px;
    z-index: 100;
}
.empty-project2{
    font-size: 13px;
    text-align: center;
    color: #323546;
    letter-spacing: -0.6px;
}
.empty-project2 image{
    display:block;
    width:70px;
    height:75px;
    margin: 183px auto 6px;
    opacity: 0.6;
}


.swiper-tab{
    width: 100%;
    /* border-bottom: 1px solid #202330; */
    text-align: center;
    height: 34px;
    
    font-weight: bold;
    background: #171d29;
}
.swiper-tab-item{
    display: inline-block;
    width: 50%;
    height: 34px;
    line-height: 34px;
    /* color: #9B9B9B; */
    font-size: 15px;
    /* background:#383B4A; */
    /* border: 1px solid #202330; */
    box-sizing: border-box;
    color: #FFFFFF;
    letter-spacing: -0.3px;
}
.active{
    background: #1166EA;
}


/* 系统消息弹窗 */
.systemNotification_box{
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    background: rgba(0,0,0,0.5);
}
.systemNotification{
    width: 280px;
    /* height: 226px; */
    position: absolute;
    z-index: 16;
    left: 50%;
    margin-left: -140px;
    top: 50%;
    margin-top: -113px;
    background: #FFFFFF;
    border-radius: 4px;
    overflow: hidden;
}
.systemNotification .title{
    width: 100%;
    height: 50px;
    position: relative;
    /* border-bottom: 1px solid #F1F3F6; */
    font-size: 15px;
    line-height: 50px;
    /* color: #4A4A4A; */
    letter-spacing: 0;
    text-align: center;
}
.systemNotification .title image{
    width: 100%;
    height: 50px;
    position: absolute;
    left: 0;
    top: 0;
}
.systemNotification .title text{
    width: 100%;
    height: 50px;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 2;
    color: #fff;
}
.systemNotification .content2{
    font-size: 13px;
    color: #9B9B9B;
    letter-spacing: 0;
    text-align: justify;
    padding: 10px 12px 35px;
    line-height: 18px;
    min-height: 180px;
}
.systemNotification .content2 .item2{
    margin-bottom: 12px;
}
.systemNotification .content2 .item2 .title2{
    color: #4a4a4a;
}

.systemNotification .foot{
    width: 100%;
    height: 35px;
    position: absolute;
    left: 0;
    bottom: 0;
    border-top: 1px solid #F1F3F6;
    display: flex;
}
.systemNotification .foot .btn{
    width: 50%;
    font-size: 15px;
    letter-spacing: 0;
    text-align: center;
    line-height: 35px;
}
.systemNotification .foot .more{
    color: #9B9B9B; 
    border-right: 1px solid #F1F3F6;
}
.systemNotification .foot .know{
    color: #0345c7;
}

.accountUpdate .title2{
    color: #4A4A4A!important;
    text-align: center!important;
    margin-bottom: 5px;
}
