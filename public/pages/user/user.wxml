
<view class="user_container" style="height: 100%">
  <view class="user_pic">
      <image class="userinfo-avatar" src="{{avatar}}" background-size="cover"></image>
      <text class="userinfo-nickname">{{realName}}</text>
  </view>
  <view class="user_info">
    <view class="user_info_title account_info">
      <view class="account_information">账户信息</view>
      <view class="user_Level">{{userLevel}}</view>
    </view>
    <view class="wrap_box">
      <view class="wrap">  
        <view class="top">  
          <canvas class="cir cir1" canvas-id="canvasArc">  
          </canvas>  
          <view class="cc">
            <view class="count">{{storage_bili}}</view>
            <view class="bili">{{storage}}GB</view>
          </view>  
          <view class="canvas_title">存储使用</view>
        </view>  
      </view> 
      <view class="wrap">  
        <view class="top">  
          <canvas class="cir cir2" canvas-id="canvasArc2">  
          </canvas>  
          <view class="cc">
            <view class="count">{{project_count}}</view>
            <view class="bili">{{project}}</view>
          </view>  
          <view class="canvas_title">项目数</view> 
        </view>  
      </view> 
      <view class="wrap">  
        <view class="top">  
          <canvas class="cir cir2" canvas-id="canvasArc3">  
          </canvas>  
          <view class="cc">
            <view class="count">{{member_count}}</view>
            <view class="bili">{{member}}</view>
          </view>  
          <view class="canvas_title">成员数</view> 
        </view>  
      </view> 
    </view>
  </view>
  <view class="user_info_title user_info_title2">
    <view class="remaining_time">
      <text>剩余时间</text>
    </view>
    <text class="time">{{end_at}}</text>
  </view>
  <!-- <view class="user_info_title">
    <text class="remaining_time">切换账号</text>
    <text class="time">></text>
  </view> -->
  <view class="user_info_title user_info_title2" bindtap="toCollection">
    <view class="remaining_time">
      <text>收藏夹</text>
      <!-- <view class="red_dot" wx:if="{{!redDot}}"></view> -->
    </view>
    <view class="list_icon">
      <image src="./img/to.png"></image>
    </view>
  </view>

  <!-- <navigator target="miniProgram" class="user_info_title user_info_title2"
    app-id="wx9f0ca9e649e69541" path="/pages/loading/loading"
    version="release" bindsuccess="bindsuccess">
      <view class="remaining_time">
        <text>创建名片</text>
      </view>
      <view class="list_icon">
        <image src="./img/to.png"></image>
      </view>
  </navigator> -->

  <view class="user_info_title user_info_title2" bindtap="toCoupon">
    <view class="remaining_time">
      <text>我的代金券</text>
      <!-- <view class="red_dot" wx:if="{{!redDot}}"></view> -->
    </view>
    <view class="list_icon">
      <image src="./img/to.png"></image>
    </view>
  </view>

  <!-- <view class="user-control">
    <button class="login-out" bindtap="loginOut">退出账号</button>
  </view> -->
</view>
