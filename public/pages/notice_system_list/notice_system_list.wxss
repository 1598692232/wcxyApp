.notice-tips{
    height: 48px;
    background: #202330;
    line-height: 48px;
    text-align: center;
}
.notice-tips-text{
    font-size: 16px;
    color: #FFFFFF;
}
.empty-notice{
    font-size: 13px;
    color: #323546;
    text-align: center;
}
.empty-notice image{
    display:block;
    width:70px;
    height:75px;
    margin: 200px auto 14px;
}

.item-system{
    box-sizing: border-box;
    width: 100%;
    height: 74px;
    padding: 0 12px;
    border-bottom: 1px solid #2F3344;
    display: flex;
    justify-content: space-between;
    line-height: 74px;
}
.item-system-left{
    font-size: 15px;
    color: #FFFFFF;
}
.item-system-right{
    font-size: 11px;
    color: #9B9B9B;
}
