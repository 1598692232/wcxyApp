/*  联系人列表展示 */
.member-drop{
	position: absolute;
	left: 0;
	top: 0;
	z-index: 666;
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,0.5);
}
.member-drop-list-show{
	width: 100%;
	height: 436px;
	background: #171d29;
	position: relative;
}
.member-drop-add{
	height: 80px;
	border-bottom: 1px solid #14161a;
	font-size: 15px;
	color: #9B9B9B;
	letter-spacing: 1.29px;
	padding: 26px 12px 0;
	display: flex;
}
.member-drop-add image{
	width: 34px;
	height: 34px;
}
.member-drop-add .text{
	line-height: 36px;
	margin-left: 12px;
	margin-top: 6px;
	flex-grow: 1;
}
.member-drop-add .text input{
	width: 100%;
	line-height: 36px;
}
.member-drop-add .btn{
	width: 60px;
	height: 34px;
	background: #0345C7;
	border-radius: 4px;
	font-size: 13px;
	line-height: 34px;
	color: #FFFFFF;
	text-align: center;
	letter-spacing: 0.66px;
}


.join-member-drop-box{
	width: 100%;
	height: 316px;
	overflow: auto;
}
.join-member-drop-box2{
	width: 100%;
	height: 384px;
	overflow: auto;
}
.join-member-drop{
	width: 100%;
	height: auto;
	/* background: #383B4A; */
}
.join-member-drop-li{
	padding: 12px 0 0;
	display: flex;
}
.join-member-drop-li .image{
	padding: 0 12px;
	display: flex;
	justify-content: center;
	align-content: center;
}
.join-member-drop-li .image image{
	width: 34px;
	height: 34px;
	border-radius: 50%;
}
.join-member-drop-li .list{
	padding-bottom: 12px;
	flex-grow: 1;
	border-bottom: 1px solid #14161a;
	text-align: left;
}
.join-member-drop-li .list .join-member-name{
	text-align: left;
	font-size: 15px;
	color: #FFFFFF;
	letter-spacing: 1.29px;
}
.join-member-drop-li .list .join-member-type{
	text-align: left;
	font-size: 12px;
	color: #9B9B9B;
	letter-spacing: 1.03px;
}
.member-drop-quit{
	position: absolute;
	left: 0;
	bottom: 0;
	width: 100%;
	height: 40px;
	font-size: 15px;
	color: #9B9B9B;
	letter-spacing: 1.29px;
	padding: 0 12px;
	line-height: 40px;
}
.member-drop-list-btn{
	width: 80px;
	height: 40px;
	background: #1A3BDE;
	border-radius: 80px;
	margin: 10px auto 0;
	display: flex;
	justify-content: center;
	align-items: center;
}
.member-drop-list-btn image{
	width: 20px;
	height: 20px;
}


/* 分享按钮 */
.share{
	position: fixed;
	left: 0;
	bottom: 0;
	width: 100%;
	height: 50px;
	padding: 0 22px;
	/* background: #303440; */
	background: #161515;
	color: #fff;
	display: flex;
	justify-content: space-between;
	line-height: 50px;
	font-size: 15px;
	z-index:100;
}
.createShare{
	background: #0345C7;
}
.upload{
  position: fixed;
  bottom: 50px;
  width:100%;
  background-color: #010101;
  height: 50px;
  z-index:1;
}
.upload .upload-button{
    width:86px;
    height:34px;
    background-color:#0345C7;
    border-radius:100px;
    font-size:14px;
    color:#FFF;
	margin-top: 8px;
	display: flex;
	align-items: center;
}
.upload .upload-button image{
    width: 20px;
    height: 20px;
    display:inline-block;
	overflow:hidden;
	margin-right: 6px;
}

