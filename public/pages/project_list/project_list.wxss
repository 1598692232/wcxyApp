.pl-container{
	width: 100%;
	position: relative;
}

.pl-list{
	width: 100%;
	box-sizing:border-box;
	background: #010101;
}
.pl-body{
	width:100%;
	padding-bottom: 50px;
}
.pl-li{
	width: 100%;
	/* margin-right: 10px; */
	/* margin-bottom: 10px; */
	padding: 8px 12px;
	overflow: hidden;
	border-bottom: 1px solid #14161A;
	position: relative;
}
.pl-li .share-btn{
	width: 25px;
	height: 25px;
	border-radius: 50%;
	position: absolute;
	right: 12px;
	bottom: 8px;
	/* background: rgba(255,255,255,0.70);
	box-shadow: 0 1px 3px 0 rgba(0,0,0,0.50); */
	border: 1px solid #979797;
	box-shadow: 0 1px 3px 0 rgba(0,0,0,0.50);
}
.pl-li .select-share{
	width: 25px;
	height: 25px;
	border-radius: 50%;
	position: absolute;
	right: 12px;
	bottom: 8px;
	background: #0345C7;
	border: none;
}
.pl-li .select-share image{
	width: 25px;
	height: 25px;
}
.pl-img-body{
	position: relative;
	float: left;
	width: 120px;
	height: 92px;
	border-radius: 4px;
	overflow: hidden;
	background: #171d29;
	/* background: #181d28; */
	/* background: #30343f; */
	/* background: #21212d; */
	/* background: #252525; */
}
.pl-show-count:before{
	content: "";
	display: block;
	width: 80px;
	height: 80px;
	background: #000;
	position: absolute;
	right: -51px;
	top:-41px;
	transform: rotate(45deg);
}
.pl-count{
	position: absolute;
	right: 10px;
	top:2px;
	display: inline-block;
	color:#fff;
	line-height: 25px;
	font-size:13px;
}
.pl-timer{
	display: inline-block;
	padding: 0 10px;
	line-height: 20px;
	position: absolute;
	right: 5px;
	bottom: 5px;
	background: rgba(0,0,0, 0.5);
	border-radius: 10px;
	font-size:20rpx;
	color:#fff;
}
.pl-img{
	max-width: 100%;
	max-height: 100%;
}
.pl-img2{
	display: flex;
	justify-content: center;
	align-items: center;
	height: 100%;
}
.pl-ext{
	position: relative;
	display: flex;
	justify-content: center;
	align-items: center;
	width: 50px;
	height: 50px;
	color: #fff;
	font-size: 12px;
	color: #fff;
}

.pl-ext image {
	position: absolute;
	left: 50%;
	top: 50%;
	margin-top: -25px;
	margin-left: -20px;
	width: 40px;
	height: 50px;
	z-index: 0;
}
.pl-ext text{
	position: relative;
	z-index: 1;
}
.pl-img2 > image{
	width: 40px;
}
.pl-des-body{
	float: left;
	margin: 8px 0 8px 8px;
}
.pl-name{
	display: inline-block;
	box-sizing: border-box;
	width: 100%;
	font-size: 15px;
	color:#666;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	color: #fff;
	/* line-height: 20px; */
	margin-top: 5px;
	font-weight: 400;
}

.pl-user{
	width: 100%;
	margin-top: 28rpx;
}

.pl-user-tou{
	float: left;
	width:30rpx;
	height: 30rpx;
	border-radius: 50%
}
.pl-text{
	/* margin-top: 10rpx; */
}

.pl-text-p{
	display: inline-block;
	margin-right: 10px; 
	float: left;
	font-size: 12px;
	color: #9B9B9B;
	line-height:1;	
}

.pl-video-icon{
	float: left;
	margin-right: 5px;
	width: 25rpx;
	height: 25rpx;
}

.pl-create-timer{
	display: inline-block;
	margin-left: 4px;
	float: left;
	line-height: 36rpx;
	color:#6c6b7b;
	font-size: 11px;
}

