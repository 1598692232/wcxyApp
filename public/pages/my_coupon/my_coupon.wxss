.my_coupon{
    width: 100%;
    padding: 24rpx;
    padding-bottom: 150rpx;
}
.my_coupon .count{
    font-size: 24rpx;
    line-height: 33rpx;
    color: #FFFFFF;
    letter-spacing: 0;
    margin-bottom: 24rpx;
}
.my_coupon .list{
    width: 702rpx;
    height: 199rpx;
    position: relative;
    margin-bottom: 20rpx;
}
.my_coupon .list image{
    display: block;
    width: 100%;
    height: 100%;
}


.top-text{
    width: 665rpx;
    height: 159rpx;
    position: absolute;
    left: 0;right: 0;
    top: 20rpx;
    margin: auto;
    display: flex;
}
.top-text .left{
    width: 226rpx;
    text-align: center;
    line-height: 165rpx;
    font-size: 50rpx;
    color: #F84B30;
    font-weight: 600;
}
.top-text .left .money{
    font-size: 25rpx;
    font-weight: 400;
}
.top-text .right{
    flex-grow: 1;
    display: flex;
    justify-content: center;
    align-items: center;
}
.top-text .right .text{
    width: auto;
    color: #F84B30;
}
.top-text .right .text1{
    font-size: 34rpx;
    line-height: 57rpx;
    letter-spacing: 2rpx;
}
.top-text .right .text2{
    font-size: 28rpx;
    line-height: 36rpx;
    letter-spacing: 1.4rpx;
    margin-top: 5rpx;
}

.tips{
    padding: 24rpx;
    position: fixed;
    left: 0;
    bottom: 0;
    font-size: 24rpx;
    color: #9B9B9B;
    letter-spacing: 1.21rpx;
    background: #010101;
}