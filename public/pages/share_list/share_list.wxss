.share-list-box{
    width: 100%;
    /* background: #14161a; */
    background: #010101;
    position: relative;
}
.share-list{
    height: 70px;
    display: flex;
    padding: 0 12px;
    position: relative;
    border-bottom: 1px solid #14161A;
}
.share-list:last-child{
    border-bottom: none;
}
.share-list .pic{
    width: 58px;
    height: 70px;
    margin-right: 12px;
    display: flex;
    justify-content: center;
    align-items: center;
}
.share-list .pic .pic-content{
    width: 58px;
    height: 44px;
    border-radius: 4px;
    background: #1166EA;
    color: #fff;
    font-size: 20px;
    line-height: 44px;
    text-align: center;
}
.share-list .pic .pic-content text{
    width: 58px;
    height: 44px;
    border-radius: 4px;
    background: #1166EA; 
}
.share-list .pic image{
    width: 58px;
    height: 44px;
    border-radius: 4px;
}

.share-list .empty_pic{
    width: 58px;
    height: 80px;
    margin-right: 12px;
    display: flex;
    justify-content: center;
    align-items: center;
}
.share-list .empty_pic .pic-content{
    width: 58px;
    height: 44px;
    border-radius: 4px;
    background:#171D29!important;
    display: flex;
    justify-content: center;
    align-items: center;
}
.share-list .empty_pic .pic-content .red{
    width: 16px;
    height: 16px;
    border-radius: 50%;
}
.share-list .empty_pic .red image{
    width: 100%;
    height: 100%;
    display: block;
}


.share-list .info{
    display: flex;
    height: 100%;
    flex-direction: column;
   justify-content: center;
    /* height: 44px; */
    /* margin-top: 22px; */
}
.share-list .info .share-list-name{
    width: 200px;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
    font-size: 13px;
    line-height: 30px;
    color: #FFFFFF;
}
.share-list .info .share-list-info{
    height: 16px;
    font-size: 11px;
    color: #9B9B9B;
    display: flex;
    line-height: 24px;
}
.share-list .info .share-list-info .text{
    line-height: 16px;
}
.share-list .info .share-list-info image{
    width: 16px;
    height: 16px;
    border-radius: 50%;
    margin-top: 1px;
}
.share-list .info .share-list-info .text{
    margin-left: 8px;
}

.number{
    position: absolute;
    right: 12px;
    top: 25px;
    font-size: 12px;
    color: #9B9B9B;
}
.number text{
    color: #fff;
}
.empty-notice{
    font-size: 13px;
    color: #323546;
    text-align: center;
    opacity: 0.6;
}
.empty-notice image{
    display:block;
    width:70px;
    height:75px;
    margin: 200px auto 14px;
}


/* 选项卡样式 */
.swiper-tab{
    width: 100%;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 9;
    /* border-bottom: 1px solid #202330; */
    text-align: center;
    height: 34px;
    
    font-weight: bold;
    background: #263044;
}
.swiper-tab-item{
    display: inline-block;
    width: 50%;
    height: 34px;
    line-height: 34px;
    /* color: #9B9B9B; */
    font-size: 15px;
    /* background:#383B4A; */
    /* border: 1px solid #202330; */
    box-sizing: border-box;
    color: #FFFFFF;
    letter-spacing: -0.3px;
}
.active{
    background: #1166EA;
}
/* 取消收藏 */
.share-list2{
    padding: 0!important;
}
.item-left-content {
    width: 100%;
    padding: 0 12px;
    display: flex;
    -webkit-transition: all 0.4s;
    transition: all 0.4s;
    -webkit-transform: translateX(90px);
    transform: translateX(90px);
    margin-left: -88px;
}
.del {
    background-color: #F84B30;
    width: 88px;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;

    font-size: 15px;
    color: #FFFFFF;
    letter-spacing: 1.29px;
    text-align: center;

    -webkit-transform: translateX(90px);
    transform: translateX(90px);
    -webkit-transition: all 0.4s;
    transition: all 0.4s;
  }
  .touch-move-active .item-left-content,
  .touch-move-active .del {
    -webkit-transform: translateX(0);
    transform: translateX(0);
  }
