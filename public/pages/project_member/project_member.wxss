.project_member{
    padding: 20px 24px;
    background: #010101;
}
.members{
    display: flex;
    justify-content: flex-start;
    flex-wrap: wrap;
}
.member_list_box{
    width: 20%;
    display: flex;
    justify-content: center;
}
.member_list{
    width: 46px;
    height: 71px;
    margin-bottom: 20px;
    position: relative;
}
.member_list>image{
    width: 46px;
    height: 46px;
    background: #FFFFFF;
    border-radius: 4px;
}
.member_list .member_name{
    width: 100%;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
    font-size: 12px;
    color: #FFFFFF;
    text-align: center;
}
.member_list .btn{
    width: 46px;
    height: 46px;
    border-radius: 4px;
    background: #0345C7;
    display: flex;
    justify-content: center;
    align-items: center;
}
.member_list .btn image{
    width: 20px;
    height: 20px;
    background: transparent!important;
}
.member_list .delete_member{
    width: 15px;
    height: 15px;
    border-radius: 50%;
    background: transparent!important;
    position: absolute;
    right: 2px;
    top: 2px;
    display: flex;
    justify-content: center;
    align-items: center;
}
.member_list .delete_member>image{
    width: 15px;
    height: 15px;
}

/* 项目按钮 */
.project_btn{
    width: 86px;
    height: 34px;
    background: #0345C7;
    border-radius: 100px;
    position: fixed;
    left: 50%;
    margin-left: -43px;
    bottom: 20px;
}
.project_btn .project_text{
    font-size: 14px;
    color: #FFFFFF;
    letter-spacing: 0.71px;
    text-align: center;
    line-height: 34px;
}